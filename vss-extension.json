{
	"manifestVersion": 1,
	"id": "ARMTTKExtension",
	"version": "1.0.2",
	"name": "Run ARM TTK Tests",
	"description": "Run Azure Resource Manager Template Test Kit tests as part of your build or release process",
	"publisher": "Sam-Cogan",
	"repository": {
	    "type": "git",
      	    "url": "https://github.com/sam-cogan/arm-ttk-extension"
	},
	"content": {
	    "details": {
		"path": "README.md"
		},
		"license": {
		"path": "LICENSE"
		}
	},
	"tags":[
	    "Azure Pipelines",
		"Azure",
		"Azure Resource Manager",
	    "Tests"
	],
	"targets": [
		{
			"id": "Microsoft.VisualStudio.Services"
			}
		],
	"icons": {
		"default": "icon.png"
	 },
	 "categories": [
               "Azure Pipelines"
	],
	"screenshots": [
		{
		  "path": "images/TestResults.png)"
		}
	  ],
	"contributions": [
		{
			"id": "RunARMTTKTests",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "task"
			}
		}
	],
	"files":[
		{
			"path": "task"			
		}
	]
}


